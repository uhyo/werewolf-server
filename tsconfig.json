{
    "compilerOptions":{
        "lib": ["dom", "es5", "es2015.core", "es2015.promise"],
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "strictNullChecks": true,
        "module":"commonjs",
        "target":"es5",
        "outDir":"dist/"
    },
    "include":[
        "db/**/*.ts", "keeper/**/*.ts", "front/**/*.ts"
    ],
    "exclude":[
        "node_modules/**/*.ts"
    ]
}



