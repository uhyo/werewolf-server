{
  "name": "werewolf-server",
  "version": "0.0.1",
  "description": "Are you a WEREWOLF?",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean build:tsc lint",
    "build:tsc": "tsc",
    "clean": "rimraf dist",
    "lint": "find keeper/ db/ front/ -name \"*.ts\" | xargs tslint --force",
    "start": "node dist/front/server",
    "watch": "npm-run-all --parallel watch:*",
    "watch:lint": "watch 'npm run lint' keeper/ db/ front/ --wait=0.3 --verbose",
    "watch:tsc": "tsc --watch"
  },
  "keywords": [
    "werewolf"
  ],
  "author": "uhyo",
  "license": "MIT",
  "dependencies": {
    "config": "^1.21.0",
    "express": "^4.14.0",
    "express-ws": "^2.0.0",
    "js-yaml": "^3.6.1",
    "redis": "^2.6.2"
  },
  "devDependencies": {
    "@types/config": "0.0.29",
    "@types/express": "^4.0.32",
    "@types/redis": "^0.12.31",
    "@types/ws": "0.0.30",
    "npm-run-all": "^3.1.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0",
    "watch": "^0.19.2"
  }
}
